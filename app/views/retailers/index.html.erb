<!doctype html>
<html>
<head>
<title>Content Store :: Retailers </title>
<!-- Latest compiled and minified CSS -->
    <!-- Header -->
    <%= render :partial => "layouts/top_header"%>
    <!-- Header Ends --> 

</head>

<body>
<!-- Aside Start-->
    <%= render :partial => "layouts/sidebar"%>
    <!-- Aside Ends-->
<!--Main Content Start -->
  <section class="content"> 
    <!-- Page Content Start --> 
    <!-- ================== -->
    <div class="wrapper container-fluid">
      <ol class="breadcrumb">
           <li><a href="#">Home</a></li>
         <li><a> Manage User Account</a></a></li>
         <li><a>Retailers </a></li>
      </ol>

      <h2 class="H2-L1">Select Publisher</h2>
       
      <%= form_tag new_retailer_path, :method => 'get', :remote => true do%>
        <div class="col-md-6">
          <div class="form-group">
            <label for="taskName" class="col-sm-4 nopadding">Select Publisher<sup class="mandatory">*</sup> </label>
            <div class="col-sm-8">
            <%= select_tag 'publisher_name',options_from_collection_for_select(Publisher.all, 'publisher_name', 'publisher_name'), :prompt => "Select publisher", :class => "form-control", style: "background-color: #eee;"%>
               
            </div>
            <div class="clearfix"></div>
          </div>
        </div>
         
        <div class="clearfix m-b-30"></div>
        <div class="row">
          <div class="col-lg-12 col-md-12 col-sm-12 colx-12 text-right">

            <%= button_tag fa_icon("plus-square-o", text: "Submit"), :class => "btn btn-sm", type: :submit %>

            <%= button_tag fa_icon("refresh", text: "Reset"), :class => "btn btn-sm" ,type: :reset%>
          </div>
        </div>
      <% end %>
    <div id="search"></div>
    </div>  
    <!-- Footer Start -->
    <%= render :partial => "layouts/footer" %>
    <!-- Footer Ends --> 
  </section>
</body>
</html>
