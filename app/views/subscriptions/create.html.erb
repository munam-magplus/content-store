        <div class="form-group">
          <label for="taskName" class="col-sm-4 control-label">Subscription ID:</label>
          <div class="col-sm-8">
            <input id="subscriptionId" class="form-control" value="System Generated" disabled="">
  		  </div>
        </div>
      </div>
      <div class="col-md-6">
        <div class="form-group">
          <label for="bookISBN" class="col-sm-4 control-label">Subscription Name:</label>
          <span class="col-sm-8">
          <input id="subscriptionName" maxlength="100" value="" placeholder="Subscription Name" class="form-control">
          </span> </div>
      </div>
	  <div class="col-md-6">
        <div class="form-group">
          <label for="bookISBN" class="col-sm-4 control-label">Publisher:</label>
          <span class="col-sm-8">
          <select name="pubCodeCombo" id="pubCodeCombo" class="form-control" onchange="publisherSelected(this.value);">
                      
                      <option value="wtbooks"> World Technology </option>
                      
                      <script language="javascript" type="text/javascript">
												publisherSelected('wtbooks');
									</script>
                      
                    </select>
          </span> </div>
      </div>
	  <div class="col-md-6">
	  <div class="form-group">
          <label for="taskName" class="col-sm-4 control-label">Subscription Description:</label>
          <div class="col-sm-8">
            <textarea name="subscriptionDesc" cols="50" rows="3" style="resize:none;" id="subscriptionDesc" class="form-control"></textarea>
  		  </div>
        </div>
		</div>
		<div class="col-md-12">
        <div class="form-group">
          <label for="bookISBN" class="col-sm-2 control-label">Subscription category:</label>
          <span class="col-sm-8">
          <label class="radio-inline">
			  <input type="radio" name="inlineRadioOptions" id="inlineRadio1" value="option1"> Online Subscription for library
			</label>
			<label class="radio-inline">
			  <input type="radio" name="inlineRadioOptions" id="inlineRadio2" value="option2"> Individual Borrow
			</label>
			<label class="radio-inline">
			  <input type="radio" name="inlineRadioOptions" id="inlineRadio3" value="option3"> Library Borrow
			</label>
          </span> </div>
      </div>
      <div class="clearfix"></div>
    </form>
