<div class="col-lg-12 col-md-12 col-sm-12 colx-12 text-right">
  <input type="button" name="button2" id="button" value="Search" onclick="getSearchDetails2();" class="submit_btn" />
</div>
<script type="text/javascript">
  function getSearchDetails2(){
    var isbn = $('#isbn').val();
    var title = $('#title').val();
    var file_name = $('#file_name').val();
    // var author_first_name = $('#author_first_name').val();
    // var author_last_name = $('#author_last_name').val();
    var publication_year = $('#publication_year').val();
    $.ajax({
      type: 'GET',
      url: '/subscriptions/title_search',
      data : { isbn: isbn, title: title, file_name: file_name, publication_year: publication_year }
    });
  }
</script>