<div class="row">
  <div class="col-md-12">
    <div class="mt10 table-demo">Article Search Results 
      <span class="pull-right">
        <div class="dropdown">
          <ul class="nav">
            <li class="drop"><i style="cursor: pointer;width: 12px;" class="fa fa-ellipsis-v" aria-hidden="true"></i>
              <ul class="dropdown-menu-right">
                <li><a href="#"><i class="fa fa-file-excel-o" aria-hidden="true"></i> Excel</a></li>
                <li><a href="#"><i class="fa fa-file-o" aria-hidden="true"></i> CSV</a></li>
                <li><a href="#"><i class="fa fa-file-pdf-o" aria-hidden="true"></i> PDF</a></li>
              </ul>
            </li>
          </ul>
        </div>
      </span>
    </div>
    <div class="table-responsive">
      <table class="table table-bordered">
        <thead>
          <tr style="font-size:13px">
            <th>Journal Code</th>
            <th>Title</td>
            <th>Content Code/DOI</th>
            <th>Article Type</th>
            <th>Volume Number</th>
            <th>Issue Type</th>
            <th>Issue/Supplement Number</th>
            <th>Contributors</th>
          </tr>
        </thead>
        <tbody>
          <% if @article.present? %>
            <% @article.each do |f| %>
              <tr>
                <td><%= f.content_code %></td>
                <td><%= f.journal_title %></td>
                <td><%= f.doi %></td>
                <td><%= f.article_type %></td>
                <td><%= f.volume_number %></td>
                <td><%= f.issue_type %></td>
                <td><%= f.issue_number %></td>
                <td></td>
              </tr>
            <% end %>
          <% else %>
            <tr>
              <td bgcolor="#FBFBFB">
                <p> Record Not Found </p> 
              </td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
  <div class="col-sm-12"> </div>
</div>