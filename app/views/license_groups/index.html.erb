<!doctype html>
<html>
<head>
<title>Content Store :: Search Result</title>
<!-- Latest compiled and minified CSS -->
<%= render :partial => "layouts/header"%>
</head>

<body>
  <section class="content"> 
    <!-- Header -->
   <%= render :partial => "layouts/top_header"%>
    <!-- Header Ends --> 
    <!-- Page Content Start --> 
    <!-- ================== -->
    <div class="wrapper container-fluid">
      <ol class="breadcrumb">
         <li><a href="#">Home</a></li>
       <li><a><%= link_to "Manage Access",manage_access_index_path%></li>
       <li><%= link_to "Search Result",license_groups_path%></li>
         <div class="pull-right hidden">
          <li><a href="#">Browse by Chapter</a></li>
          <li><a href="#">Browse by Book </a></li>
         </div>
        </ol>
      <h2 class="H2-L1">Search License Group</h2>

      <%= form_tag search_license_groups_path, :method => 'get', :remote => true, :class => "form-horizontal form-bg" do  %>
        <div class="col-md-6">
          <div class="form-group">
            <label for="taskName" class="col-sm-4 control-label">Select Publisher<sup class="mandatory">*</sup> </label>
            <div class="col-sm-8">
              <%= select_tag 'publisher',options_from_collection_for_select(Publisher.all, 'publisher_name', 'publisher_name'), :prompt => "Select publisher", :class => "form-control"%>
            </div>
          </div>
        </div>
        <div class="col-md-6">
          <div class="form-group">
            <label for="taskName" class="col-sm-4 control-label">License Group ID</label>
            <div class="col-sm-8">
              <%= text_field_tag :license_group_id,"",:class => "form-control" %>
            </div>
          </div>
        </div>
        <div class="col-md-6">
          <div class="form-group">
            <label for="taskName" class="col-sm-4 control-label">License Group Name</label>
            <div class="col-sm-8">
              <%= text_field_tag :license_group_name,"",:class => "form-control" %>
            </div>
          </div>
        </div>
        <div class="col-md-6">
          <div class="form-group">
            <label for="taskName" class="col-sm-4 control-label">License Name</label>
            <div class="col-sm-8">
              <%= text_field_tag :license_name,"",:class => "form-control" %>
            </div>
          </div>
        </div>
        <div class="col-md-6">
          <div class="form-group">
            <label for="taskName" class="col-sm-4 control-label">Created By</label>
            <div class="col-sm-8">
              <%= text_field_tag :created_by,"",:class => "form-control" %>
            </div>
          </div>
        </div>
        <div class="col-md-6">
          <div class="form-group">
            <label for="bookISBN" class="col-sm-4 control-label">Saleability</label>
            <div class="col-sm-8">
             <%= select_tag 'license_for_sale',options_for_select([['Select',''], [' For Sale',:y], ['Not For Sale',:n]]), :prompt => "select status", :class => "form-control" %>
            </div>
          </div>
          <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 colx-12 text-right">
              <%= button_tag fa_icon("search", text: "Search"), :class => "btn btn-sm", type: :submit, :id => "submit" %>
              <%= button_tag fa_icon("refresh", text: "Reset"), :class => "btn btn-sm" ,type: :reset%>
            </div>
          </div>
      <% end %>
      <div class="clearfix m-b-30"></div> 
      <div id="search_result"></div>
    </div>  
    <!-- Footer Start -->
    <%= render :partial => "layouts/footer" %>
    <!-- Footer Ends --> 
  </section>
</body>
</html>