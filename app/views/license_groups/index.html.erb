                <td height="25" width="171">Publisher</td>
                <td height="25" width="414">
                  <%= collection_select(nil, :id, Publisher.all, :id, :pub_name) %>
                </td>
              </tr>
              <tr>
                <td height="25" width="149">License Group ID</td>
                <td height="25">
                  <%= text_field_tag :lic_group_ID %>
                </td>
              </tr>
              <tr>
                <td height="25">License Group Name</td>
                <td height="25" width="436">
                  <%= text_field_tag :lic_group_name %>
                </td>
              </tr>
              <tr>
                <td height="25">License Name</td>
                <td height="25" width="436">
                  <%= text_field_tag :lic_name %>
