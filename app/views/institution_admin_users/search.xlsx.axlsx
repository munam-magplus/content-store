wb = xlsx_package.workbook
wb.add_worksheet(name: "Institution Admin User") do |sheet|
	sheet.add_row [ "Contact Name", "Email Id", "Status", "User Name", "User Role", "Institution Name" ]
	@result.each do |f|
 		sheet.add_row [ f.first_name + f.last_name, f.email, f.status,   f.user_name, f.role, f.institution_name ]
	end
end


