<html>
<body class = "body1">
  <%= render :partial => '/layouts/header' %>
  <table class = "center">
    <tr>
      <td class = "page_head"> Create Institution</td>
    </tr> 
    <tr> 
      <%= form_for(:institution_account, url:  institution_account_index_path) do |f| %>
        <td align = "left" >
          <table border = "0", cellpadding = "10", cellspacing = "0">
            <tbody>
              <tr>
                <b> Details </b>
                <td height = "25"> Select Publisher</td>
                <td align = "left">
                  <%= f.select 'publisher',options_from_collection_for_select(Publisher.all, 'id', 'publisher_name') %>
                </td>
              </tr>
              <tr>
                <td align = "left", height = "25"> Institution ID</td>
                <td align = "left", height = "25">
                  <%= f.text_field :institution_id %>
                </td>
                <td align = "left", height = "25"> Lib./Dept. Name</td>
                <td align = "left">
                  <%= f.text_field :libary_name %>
                </td>
              </tr>
              <tr>
                <td align = "left", height = "25"> Institution Name</td>
                <td align = "left">
                  <%= f.text_field :institution_name %>
                </td>
                <td height = "25"> Status </td>
                <td height = "25">
                </td>
              </tr>
            </tbody>
          </table>
          <table border = "0", cellpadding = "10", cellspacing = "0">
            <tbody>
              <tr>
                <b> Branding Options</b>
                <td align = "left", height = "25"> Display Name</td>
                <td align = "left">
                  <%= f.text_field :display_name%>
                </td>
              </tr>
              <tr>
                <td align = "left", height = "25"> Logo</td>
                <td align = "left">
                  <%= f.file_field :logo%>
                </td>
              </tr>
            </tbody>
          </table>
          <table border = "0", cellpadding = "10", cellspacing = "0">
            <tbody>
              <%= f.fields_for :institution_acc_billing_address do |builder| %>
                <tr>
                  <b> Billing Address </b>
                  <td align = "left", height = "25"> Contact First Name</td>
                  <td align = "left">
                    <%= builder.text_field :first_name %>
                  </td>
                  <td align = "left", height = "25"> parent id</td>
                  <td align = "left">                  
                  </td>
                  <td align = "left", height = "25"> Contact Last Name</td>
                  <td align = "left">
                    <%= builder.text_field :last_name %>
                  </td>
                </tr>
                <tr>
                  <td align = "left", height = "25"> Phone</td>
                  <td align = "left">
                    <%= builder.text_field :phone %>
                  </td>
                  <td align = "left", height = "25"> Email</td>
                  <td align = "left">
                    <%= builder.text_field :email %>
                  </td>
                </tr>
                <tr>
                  <td align = "left", height = "25"> Address</td>
                  <td align = "left">
                    <%= builder.text_field :address %>
                  </td>
                  <td align = "left", height = "25"> Address Line 2</td>
                  <td align = "left">
                    <%= builder.text_field :address_line2 %>
                  </td>
                </tr>
                <tr>
                  <td align = "left", height = "25"> Address Line 3</td>
                  <td align = "left">
                    <%= builder.text_field :address_line3 %>
                  </td>
                  <td align = "left", height = "25"> City / Town</td>
                  <td align = "left">
                    <%= builder.text_field :city %>
                  </td>
                </tr>
                <tr>
                  <td align = "left", height = "25"> State / Province</td>
                  <td align = "left">
                    <%= builder.text_field :state %>
                  </td>
                  <td align = "left", :height ="25"> Postal Code</td>
                  <td align = "left">
                    <%= builder.text_field :postal_code %>
                  </td>
                </tr>
                <tr>
                  <td align = "left", height = "25"> Country </td>
                  <td align = "left">                
                  </td>
                </tr>
                <tr>
                  <td align = "left", height = "25"> Comments</td>
                  <td align = "left">
                    <%= builder.text_area :comments %>
                </tr>
              <% end %>
              <tr>
                <td align = "left">  
                  <%= f.submit "Submit" %>
                </td>
              </tr>
            </tbody>
          </table>
        </td>
      <% end %>
    </tr>
  </table>
</body>
</html>

