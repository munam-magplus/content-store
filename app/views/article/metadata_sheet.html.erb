    <h2 class="H2-L1">Upload Article Metadata File</h2>
    <form class="form-horizontal form-bg" role="form">
            <%= form_tag metadata_sheet_subject_groups_path, :method => 'get' do %>
      
      <div class="col-md-12">
        <div class="form-group">
          <label for="bookISBN" class="col-sm-2 control-label">Select Language:</label>
          <span class="col-sm-4">
          <%= select_tag(:JOC_CODE, options_for_select([['Select',0],[' Dansk',1],['Polish',2],['Latvian',3],['English',4]]))  %>
          </span> 
        </div>
      </div>
    <div class="col-md-12">
        <div class="form-group">
          <label for="bookISBN" class="col-sm-2 control-label">File Name:</label>
          <span class="col-sm-4">
         <div class="file-placeholder">
             
            <input type="file" class="fileUpload" >
            
            <div class="file-browse">
              <input type="file" name="uploadMetadataFile" size="60" value="" onkeydown="return false;" onkeypress="return false;" id="uploadMetadataFile" class="text_field">
             <span class="file-browse-txt">Select somes files </span>
             <span class="browse">Browse</span>
            </div>
           </div>
          </span> 
     </div>
      </div>
      <%= submit_tag "Upload" %>
                <%= button_tag "Cancel", type: :reset %>
      <div class="clearfix"></div>
    </form>
    <div class="clearfix m-b-30"></div>
  <div class="row">
  <div class="col-lg-12 col-md-12 col-sm-12 colx-12 text-right">
