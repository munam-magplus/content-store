    <button type="submit" class="btn btn-sm" onclick="javascript:showDiv();"><i class="fa fa-cloud-upload" aria-hidden="true"></i> &nbsp;Upload</button>
        <button type="reset" class="btnBlack btn-sm" onclick="javascript:hideDiv();"><i class="fa fa-refresh" aria-hidden="true"></i> &nbsp;Clear</button>
