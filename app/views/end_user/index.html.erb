<html>
  <body class = "body1">
    <%= render :partial => '/layouts/header'%>
    <div class="div1">
      <h1>Search End Users</h1>
      <table border="0" cellpadding="10" cellspacing="0">
        <tbody>
          <%= form_tag end_user_search_path, :method => 'get' do  %>
            <tr>
              <td>Publisher*</td>
              <td><%= text_field_tag :publisher, params[:publisher] %></td>
            </tr> 
            <tr>
              <td>Email</td>
              <td>
                <%= text_field_tag :email, params[:email] %>
             </td>
            </tr> 
            <tr>
              <td>Last Name</td>
              <td>
                <%= text_field_tag :last_name, params[:last_name] %>
             </td>
            </tr> 
            <tr>
              <td>Country</td>
              <td>
                <%= text_field_tag :country, params[:country] %>
             </td>
            </tr> 
            <tr>
              <td></td>
              <td><%= submit_tag "Search" %>
                  <%= button_tag "Reset", type: :reset %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </body>
</html>

