<html>
  <body class = "body1">
    <%= render :partial => '/layouts/header'%>
    <div class="div1">

      <h1>Search End Users</h1>
      <table border="0" cellpadding="10" cellspacing="0">
        <tbody>
          <%= form_for :subject_group do |f| %>
            <tr>
              <td>Select Publisher*</td>
              <td><%= f.collection_select(:PUB_PUBLISHER_ID, Publisher.all, :id, :PUB_NAME) %></td>
             
            </tr> 
            <tr> 
              <td>Email</td>
              <td><%= f.text_field :SUG_GROUP_DESC %></td>
            </tr> 
            <tr> 
              <td>Last Name </td>
              <td><%= f.text_field :SUG_GROUP_CODE %></td>
            </tr>
            <tr>
              <td>Country</td>
              <td><%= f.collection_select :COI_COUNTRY_ID, Countryname.all, :id, :CON_COUNTRY_NAME %></td>
            </tr> 
            <tr> 
             <td>License ID</td>
              <td><%= f.text_field :SUG_GROUP_CODE %></td>
            </tr>
            <tr>
            	<td></td>
            	<td><%= f.submit "Search" %>
            		<%= f.button "Reset", type: :reset %></td>
       		</tr>


          <% end %>
        </tbody>
      </table>
      
    </div>
  </body>
</html>
