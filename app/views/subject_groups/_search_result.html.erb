<body class="body1"></body>
<%= render :partial => '/layouts/header' %>
<div class="div1">
	<h1>Subject Group List</h1>
  <table bgcolor="#cccccc" border="0" cellpadding="8" cellspacing="1" width="70%"></table>
  <table bgcolor="#cccccc" border="0" cellpadding="8" cellspacing="1" class="center" width="100%">
    <tbody>
      <tr>
        <td class="tableheader"> Subject Group Name </td>
        <td class="tableheader"> Subject Group Code </td>
        <td class="tableheader"> Subject Group Level </td>
      </tr>
		      <% if @subject_group.present? %>
	      <% @subject_group.each do |f| %>
	      <tr>
	        <td bgcolor="#FBFBFB">
	          <%= f.subject_group_name %>
	        </td>
	        <td bgcolor="#FBFBFB">
	          <%= f.subject_group_code %>
	        </td>
	        <td bgcolor="#FBFBFB">
	          <%= f.subject_group_level %>
	        </td>
	      </tr>
	      <% end %>
	      <% else %>
	        <tr>
	          <td bgcolor="#FBFBFB">
	            <p> Record Not Found </p> 
	          </td>
	        </tr>
	      <% end %>
      </tr>
    </tbody>
  </table>
</div>
<%= render :partial => '/layouts/footer'%>