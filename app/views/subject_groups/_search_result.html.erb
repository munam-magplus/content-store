<div class="row">
  <div class="col-md-12">
    <div class="mt10 table-demo">Search Result 
      <span class="pull-right">
        <div class="dropdown">
          <ul class="nav">
            <li class="drop"><i style="cursor: pointer;width: 12px;" class="fa fa-ellipsis-v" aria-hidden="true"></i>
              <ul class="dropdown-menu-right">
                <li><a href="#"><i class="fa fa-file-excel-o" aria-hidden="true"></i> Excel</a></li>
                <li><a href="#"><i class="fa fa-file-o" aria-hidden="true"></i> CSV</a></li>
                <li><a href="#"><i class="fa fa-file-pdf-o" aria-hidden="true"></i> PDF</a></li>
              </ul>
            </li>
          </ul>
        </div>
      </span>
    </div>
    <div class="table-responsive">
      <table class="table table-bordered">
        <thead>
          <tr style="font-size:13px">
            <th>Subject Name</th>
            <th>Subject Code</th>
            <th>Subject Level</th>
            <th>Action</th>
          </tr>
        </thead>
        <tbody>
          <% if @subject_group.present? %>
	      		<% @subject_group.each do |f| %>
              <tr>
                <td><%= f.subject_group_name %></td>
                <td><%= f.subject_group_code %></td>
                <td><%= f.subject_group_level %></td>
                <td><%= link_to 'Associate Books',associate_book_subject_groups_path(f.publisher_id, subject_group_name: f.subject_group_name, subject_group_id: f.id) %></td>
              </tr>
            <% end %>
          <% else %>
            <tr>
              <td bgcolor="#FBFBFB">
                <p> Record Not Found </p> 
              </td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
  <div class="col-sm-12"> </div>
</div>