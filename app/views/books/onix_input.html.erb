<html>
  <body class = "body1">
    <div class="div1">
      <table>
        <tr>
          <h1 class="page_head">Upload Onix File (in XML) </h1>
        <tr>
      </table>
      <%= form_tag metadata_sheet_subject_groups_path, :method => 'get' do %>
        <table border="0" cellpadding="10" cellspacing="0">
          <tbody>
            <tr>
              <td>File Name</td>
            </tr>
            <tr>
              <td>
               <input type="file" name="uploadMetadataFile" size="60" value="" onkeydown="return false;" onkeypress="return false;" id="uploadMetadataFile" class="text_field">
              </td>
            </tr>
            <tr>
              <td>
                <%= submit_tag "Upload" %>
                <%= button_tag "Cancel", type: :reset %>
              </td>
            </tr>
          </tbody>
        </table>
      <% end %>
    </div>
  </body>
</html>