<html>
  <body class = "body1">
    <%= render :partial => '/layouts/header'%>
    <div class="div1">
      <table>
        <tr>
          <h1 class="page_head">Upload Metadata File </h1>
        <tr>
      </table>
      <%= form_tag metadata_sheet_subject_groups_path, :method => 'get' do %>
        <table border="0" cellpadding="10" cellspacing="0">
          <tbody>
            <tr>
              <td>Select Language </td>
              <td><%= select_tag(:JOC_CODE, options_for_select([['Select',0],[' Dansk',1],['Polish',2],['Latvian',3],['English',4]]))  %></td>
            </tr>
            <tr><td>File Name</td></tr>
            <tr><td><input type="file" name="uploadMetadataFile" size="60" value="" onkeydown="return false;" onkeypress="return false;" id="uploadMetadataFile" class="text_field"></td></tr>
            <tr>
              <td>
                <%= submit_tag "Upload" %>
                <%= button_tag "Cancel", type: :reset %>
              </td>
            </tr>
          </tbody>
        </table>
        <table>
          <tr>
            <td>
              <table border="0" cellspacing="0" cellpadding="0">
                  <tr>
                    <td><a href="downloadTemplate.do?actionType=template&templateCategory=metadata&pub="><img src="images/free-download.gif" border="0" /></a></td>
                    <td width="8">&nbsp;</td>
                    <td><a href="downloadTemplate.do?actionType=template&templateCategory=metadata&pub=">
                    Download template
                  </a></td>
                  </tr>
              </table>
            </td>
          </tr>
        </table>
      <% end %>
    </div>
    <%= render :partial => '/layouts/footer'%>
  </body>
</html>