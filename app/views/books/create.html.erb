<% title "New Book"%>

<% form_for @book do |f| %>
		<%= render 'primary_content_info'%>
		<%= render 'contributors'%>
		<%= render 'content_pricing'%>
		<%= render 'content_access_rules'%>
		<%= render 'seo_config'%>
		<p><%= f.submit "Submit" %></p>
	<% end %>

<p><%= link_to "Back to list",book_path %></p>

<html>
	<body class = "body1">
			<%= render :partial => '/layouts/header'%>
		<div class="div1">
			<%= render :partial => '/books/primary_content_info'%>
		</div>
		<%= render :partial => '/layouts/footer'%>
	</body>
</html>
