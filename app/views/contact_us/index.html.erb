<p id="notice"><%= notice %></p>

<h1>Contact Us</h1>

<table>
  <thead>
    <tr>
      <th>Subject</th>
      <th>Topic</th>
      <th>Phone number</th>
      <th>Fax number</th>
      <th>Comments</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contact_us.each do |contact_u| %>
      <tr>
        <td><%= contact_u.subject %></td>
        <td><%= contact_u.topic %></td>
        <td><%= contact_u.phone_number %></td>
        <td><%= contact_u.fax_number %></td>
        <td><%= contact_u.comments %></td>
        <td><%= link_to 'Show', contact_u %></td>
        <td><%= link_to 'Edit', edit_contact_u_path(contact_u) %></td>
        <td><%= link_to 'Destroy', contact_u, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contact U', new_contact_u_path %>
