%body{:class=>"body1"}
= render :partial => '/layouts/header'
%div{:class=>"div1"}
  %table{:bgcolor => "#cccccc", :border => "0", :cellpadding => "8", :cellspacing => "1", :width => "70%"}
  %table{:bgcolor => "#cccccc", :border => "0", :cellpadding => "8", :cellspacing => "1", :width => "100%", :class => "center"}
    %tbody
      %tr
        %td.tableheader Publisher Code
        %td.tableheader Publisher Name
        %td.tableheader Contact Name
        %td.tableheader Contact Email
      %tr
        - if @publisher.present?
          - @publisher.each do |pub|
            %td{:bgcolor => "#FBFBFB"}
              = link_to pub.PUB_CODE
            %td{:bgcolor => "#FBFBFB"}
              = pub.PUB_NAME
            %td{:bgcolor => "#FBFBFB"}
              = pub.publisher_contact.PUC_CONTACT_FNAME
            %td{:bgcolor => "#FBFBFB"}
              = pub.publisher_contact.PUC_CONTACT_EMAIL
        - else
          %td{:bgcolor => "#FBFBFB"} -
          %td{:bgcolor => "#FBFBFB"} -
          %td{:bgcolor => "#FBFBFB"} -
          %td{:bgcolor => "#FBFBFB"} -
          %td{:bgcolor => "#FBFBFB"} -
          %td{:bgcolor => "#FBFBFB"} -
          %td{:bgcolor => "#FBFBFB"} -
          %td{:bgcolor => "#FBFBFB"} -