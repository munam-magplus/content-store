<div class="row">
  <div class="col-md-12">
      <div class="mt10 table-demo">Search Result <span class="pull-right">
                      <div class="dropdown">
                        <ul class="nav">
                          <li class="drop"><i style="cursor: pointer;width: 12px;" class="fa fa-ellipsis-v" aria-hidden="true"></i>
                            <ul class="dropdown-menu-right">
                              <li><a href="#"> <i class="fa fa-file-excel-o" aria-hidden="true"></i> Excel</a></li>
                              <li><a href="#"> <i class="fa fa-file-o" aria-hidden="true"></i> CSV</a></li>
                              <li><a href="#"> <i class="fa fa-file-pdf-o" aria-hidden="true"></i> PDF</a></li>
                            </ul>
                          </li>
                        </ul>
                      </div>
                      </span></div>
      <div class="table-responsive">
        <table class="table table-bordered">
          <thead>
            <tr style="font-size:13px">
              <th>Publisher Code</th>
              <th>Publisher Name</th>
              <th>Contact Name</th>
              <th>Contact Email</th>
               
            </tr>
          </thead>
          <tbody>
            <%if @publisher.present? %>
              <% @publisher.each do |pub| %>
  	  <tr>
                <td><%= link_to pub.publisher_code,edit_publisher_path(pub) %></td>
                <td ><%= pub.publisher_name %></td>
                <td ><%= pub.contact_first_name %></td>
                <td ><%= pub.contact_email %></td>
             </tr>
             <%end%>
            <%end%>
          </tbody>
        </table>
      </div>
    </div>
    <div class="col-sm-12"> </div>
  </div>
</div>