<body class="body1"></body>
<%= render :partial => '/layouts/header' %>
<div class="div1">
  <table bgcolor="#cccccc" border="0" cellpadding="8" cellspacing="1" width="70%"></table>
  <table bgcolor="#cccccc" border="0" cellpadding="8" cellspacing="1" class="center" width="100%">
    <tbody>
      <tr>
        <td class="tableheader">Publisher Code</td>
        <td class="tableheader">Publisher Name</td>
        <td class="tableheader">Contact Name</td>
        <td class="tableheader">Contact Email</td>
      </tr>
      <tr>
        <% if @publisher.present? %>
          <% @publisher.each do |pub| %>
            <td bgcolor="#FBFBFB">
              <%= link_to pub.pub_code %>
            </td>
            <td bgcolor="#FBFBFB">
              <%= pub.pub_name %>
            </td>
            <td bgcolor="#FBFBFB">
              <%= pub.contact_first_name %>
            </td>
            <td bgcolor="#FBFBFB">
              <%= pub.contact_email %>
            </td>
          <% end %>
        <% else %>
          <td bgcolor="#FBFBFB">-</td>
          <td bgcolor="#FBFBFB">-</td>
          <td bgcolor="#FBFBFB">-</td>
          <td bgcolor="#FBFBFB">-</td>
          <td bgcolor="#FBFBFB">-</td>
          <td bgcolor="#FBFBFB">-</td>
          <td bgcolor="#FBFBFB">-</td>
          <td bgcolor="#FBFBFB">-</td>
        <% end %>
      </tr>
    </tbody>
  </table>
</div>
