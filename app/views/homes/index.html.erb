<!--Main Content Start -->
  <section class="content">
    <!--Nyresearch Start-->
    <% if @publisher.domain_name == 'nyresearchpress'%>
      
      <% unless @publisher.theme_name.blank? %>
        <% if @publisher.theme_name== 'theme1' %>
          <%= render :partial => "header_theme1"%>
        <% elsif @publisher.theme_name== 'theme2' %>
          <%= render :partial => "header_theme2"%>   
        <% end %>
      <% else %>
           <%= render :partial => "nysearch_header"%>
      <% end %>  
    
      <div class="container-fluid">
        <div class="row"> 
          <div class="col-sm-4 col-md-3 ">
            <% if @publisher.theme_name== 'theme1' %>
              <%= render :partial => "sidebar_theme1"%>
            <% end %>  
          </div>
        
          <div class="col-sm-8 col-md-9 mar-b50" >
            <div id= 'pub_book'>
              <h1 class="sectionHeading">Featured Books</h1>
                <%= render :partial => 'publisher_book' %>
            </div>
          </div>   
        </div>
      </div>

    <!-- Page Content Start --> 
    <!-- ================== -->
    <div class="clearfix m-b-30"></div>
    <!-- <h2 class="H2-L1">Publisher's Books</h2> -->
    </div>

  <% elsif @publisher.domain_name == 'syrawoodpublishinghouse' %> 
    <%= render :partial => "header_theme2"%>
    <div class="container-fluid">
        <div class="row"> 
          <div class="col-sm-8 col-md-9 mar-b50" >
            <div id= 'pub_book'>
              <h1 class="sectionHeading">Featured Books</h1>
                <%= render :partial => 'publisher_book' %>
            </div>
          </div>   
        </div>
      </div>
  <% end %> 

   <!--Nyresearch End -->
  </section>

<!-- Footer Start -->
    <%= render :partial => "homes/publisher_footer"%>
    <!-- Footer Ends -->