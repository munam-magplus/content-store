<% if @books.present? %>
  <% @books.each do |f| %>
    <div class="col-sm-3">
      <div class="result media-result ebook">
        <%= link_to image_tag("http://www.#{@publisher.domain_name}.com/static/templates/template1/#{@publisher.publisher_code}/thumbnails/#{f.isbn}-T-FC.jpg", class: 'border-img'),books_description_homes_path(:book_id => f.id) %>
        <div class="details">
          <h3 class="title"><%= link_to truncate(f.book_title, length: 18), books_description_homes_path(:book_id => f.id) %>
          </h3>
          <h4 class="author">
            <% f.books_contributors.each do |book_contributor| %>
              <%= book_contributor.first_name.presence || ""%>&nbsp<%= book_contributor.last_name.presence || ""%>
            <%end%>
          </h4>  
        </div>
      </div>
    </div>
  <% end %>
<% end %>

<div class="col-md-12 mt20">
  <div class="pull-right">
    <div class="pagination-frontend">
      <nav aria-label="Page navigation" style="clear:both;"> 
        <% #unless  @publisher.publisher_name == "theachiverspress" %>
          <%#= will_paginate @books,renderer: BootstrapPagination::Rails %>
        <%# end %>
      </nav>  
    </div>
  </div>
  <div class="clearfix"></div>
  </div>
</div> 







