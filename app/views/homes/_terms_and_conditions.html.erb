<% if @publisher.domain_name == "syrawoodpublishinghouse" 	 %>
  <div class="container" style="padding-bottom:90px;">
	  <div class="row">
	    <div class="col-md-12">
	    	<div class="border_wrapper padding10px mtb25">
	    	<h2 class="heading1 border_bottom mb20">TERMS & CONDITIONS </h2>
	      <div class="termcondition">
	      	<% if @publisher_terms.present? %>
						<p class="mt20"><%= @publisher_terms.html_safe %></p>
					<%end%>
				</div>
			</div>
	    </div>
  	</div>
	</div>
<% else %>
<div class="row">
	<div class="col-sm-12">
		<div class="border_wrapper padding10px mt5">
			<h1 class="heading1 sectionHeading border_bottom ">TERMS & CONDITIONS</h1>
  			<div class="clearfix"></div>
  			<div class="mt20" id="tcHead">
    			<p>
    				<%if @publisher_terms.present? %>
			  			<div class="wrapper container-fluid">
			    			<%= @publisher_terms.html_safe %>
			  			</div>
					<%end%>
    			</p>
    			<div class="clearfix"></div>
  			</div>
			<% end %>
		</div>
	</div>
</div>

