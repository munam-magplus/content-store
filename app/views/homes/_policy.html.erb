<% if @publisher.domain_name == "mlbooksinternational" %>
	<div class="container" >
	  <div class="row">
	    <div class="col-sm-12">
	      <div class="border_wrapper padding10px mt5">
	        <h1 class="heading1 border_bottom "> Privacy Policy </h1>
	        <div class="clearfix"></div>
	        <p class="mt20">
	        	<% if @publisher_policy.present? %>
						  <div class="wrapper container-fluid">
						    <%= @publisher_policy.html_safe %>
						  </div>
						<%end%>
					</p>
	      </div>
	    </div>
	  </div>
	</div>

<% elsif @publisher.domain_name == "syrawoodpublishinghouse" 	 %>
  <div class="container" style="padding-bottom:90px;">
	  <div class="row">
	    <div class="col-md-12 mt30">
	    	<h2 class="heading1 border_bottom mb20">Privacy Policy</h2>
	      <div class="termcondition">
	      	<% if @publisher_policy.present? %>
	 					<p class="mt20"><%= @publisher_policy %></p>
					<%end%>
				</div>
	    </div>
  	</div>
	</div>
	
<% else %>
	<h1 class="sectionHeading">Privacy Policy</h1>
	<div class="row placeholders">
		<% if @publisher_policy.present? %>
		  <div class="wrapper container-fluid">
		    <%= @publisher_policy.html_safe%>
		  </div>
		<%end%>
	</div>
<% end %>